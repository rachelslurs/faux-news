root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[{package.json,.circle.yml, .babelrc, .eslintrc}]
indent_style = space
indent_size = 2

[*.{css,scss,less,variables,overrides}]
indent_style = space
indent_size = 2
